Renders hybrid consul network policy:
  1: |
    apiVersion: networking.k8s.io/v1
    kind: NetworkPolicy
    metadata:
      annotations:
        helm.sh/chart: basic-config-0.2.3
      labels:
        app.kubernetes.io/managed-by: Helm
        app.mintel.com/env: local
        app.mintel.com/region: local
        name: test-app-allow-consul-proxy
      name: test-app-allow-consul-proxy
      namespace: test-namespace
    spec:
      ingress:
        - from:
            - namespaceSelector:
                matchLabels:
                  kubernetes.io/metadata.name: hybrid-consul
          ports:
            - port: 20000
              protocol: TCP
      podSelector:
        matchLabels:
          app.kubernetes.io/part-of: null
