role can access awsapigateway objects:
  1: |
    apiVersion: rbac.authorization.k8s.io/v1
    kind: ClusterRole
    metadata:
      annotations:
        helm.sh/chart: aws-api-gateway-operator-0.1.2
      labels:
        app.kubernetes.io/component: app
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: aws-api-gateway-operator
        app.kubernetes.io/part-of: aws-api-gateway-operator
        app.mintel.com/env: ${CLUSTER_ENV}
        app.mintel.com/owner: SRE
        app.mintel.com/region: ${CLUSTER_REGION}
        name: aws-api-gateway-operator
      name: aws-api-gateway-operator-role
    rules:
      - apiGroups:
          - mintel.cloud
        resources:
          - awsapigateways
        verbs:
          - create
          - delete
          - get
          - list
          - patch
          - update
          - watch
      - apiGroups:
          - ""
        resources:
          - events
        verbs:
          - create
          - patch
      - apiGroups:
          - ""
        resources:
          - pods
        verbs:
          - get
          - list
          - watch
      - apiGroups:
          - ""
        resources:
          - nodes
          - namespaces
          - endpoints
        verbs:
          - get
          - list
          - watch
